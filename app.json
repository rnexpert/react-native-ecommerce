{
  "expo": {
    "name": "ecommerce",
    "description": "Simple E-Commerce App Powered By React Native and Marketcloud",
    "slug": "ecommerce",
    "privacy": "public",
    "sdkVersion": "20.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#00897B",
    "icon": "./assets/images/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rizkiandrianto.ecommerce"
    },
    "android": {
      "package": "com.rizkiandrianto.ecommerce"
    }
  }
}
